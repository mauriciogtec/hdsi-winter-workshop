        "            try:\n",
        "                if tool_call.get(\"name\") == \"calc_tool\":\n",
        "                    params = tool_call[\"parameters\"]\n",
        "                    result = calc_tool(**params)\n",
        "                    memory.append(f\"[Tool Result] {result}\")\n",
        "\n",
